#pragma once

#include <type_traits>

namespace msd {

// forward
template <typename T>
constexpr T&& forward(typename remove_reference<T>::type& val) {
    return static_cast<T&&>(val);
}
template <typename T>
constexpr T&& forward(typename remove_reference<T>::type&& val) {
    return static_cast<T&&>(val);
}

// move
template <typename T>
constexpr typename remove_reference<T>::type&& move(T&& val) noexcept {
    return static_cast<typename remove_reference<T>::type&&>(val);
}

} // namespace msd
